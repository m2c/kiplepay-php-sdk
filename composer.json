{
    "name": "greenpacket/kiple-sdk",
    "description": "Greenpacket Kiple Verify PHP SDK",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "symfony/http-foundation": "^4.0",
        "symfony/event-dispatcher": "^4.0"
        
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evans",
            "email": "evans.yang@greenpacket.com.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Greenpacket\\Kiple\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greenpacket\\Kiple\\Tests\\": "tests"
        }
    }
}
