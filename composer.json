{
    "name": "greenpacket/kiplepay",
    "description": "The SDK mainly shows how to access the kiplepay gateway.",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": ">=1.24",
        "symfony/http-foundation": ">=2.8 ",
        "symfony/event-dispatcher": ">=2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evans",
            "email": "evans.yang@greenpacket.com.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Greenpacket\\KiplePay\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greenpacket\\KiplePay\\Tests\\": "tests"
        }
    }
}
